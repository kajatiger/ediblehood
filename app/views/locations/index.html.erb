<h2>Listing Locations</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locations.each do |location| %>
      <tr>
        <td><%= location.title %></td>
        <td><%= image_tag location.image_url, height: 100%></td>
        <td><%= location.email %></td>
        
        <td>  </td>
        <td><%= location.latitude %></td>

        <td><%= link_to 'Show', location, class: "btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_location_path(location), class: "btn btn-default" %></td>
        <td><%= link_to 'Destroy', location, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-warning" %></td>
      </tr>
    <% end %>
  </tbody>
</table>




